<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Intex2</title>
    <link href="~/css/site.css" rel="stylesheet" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    
</head>
<body>
    <header id="header" class="fixed-top header-inner-pages">
        <div class="container d-flex align-items-center justify-content-between">
            <h1 class="logo"><a asp-area="" asp-controller="Home" asp-action="Index">FAG EL-GAMOUS</a></h1>
            
            <nav id="navbar" class="navbar">
                <ul>
                    <li>
                        <a class="nav-link scrollto" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                    </li>
                    <li>
                        <a class="nav-link scrollto" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                    </li>
                    <li>
                        <a class="nav-link scrollto" asp-area="" asp-controller="Home" asp-action="DisplayList">Summary & Records</a>
                    </li>
                    <li>
                        <a class="nav-link scrollto" asp-area="" asp-controller="Home" asp-action="Supervised">Supervised</a>
                    </li>
                    <li>
                        <a class="nav-link scrollto" asp-area="" asp-controller="Home" asp-action="Unsupervised">Unsupervised</a>
                    </li>
                    <li>
                        <partial name="_LoginPartial" class="nav-link scrollto" />
                    </li>
                                           
                    
                </ul>
                


            </nav>
        </div>
    </header>
    <div>
        <main role="main">
            @RenderBody()
        </main>
    </div>

    <!--Add Cookie Notification-->
    <partial name="_CookieConsentPartial" />

    <!-- ======= Footer ======= -->
    <footer id="footer">
        <div class="footer-top">
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-xl-10">
                        <div class="row">

                            <div class="col-lg-3 col-md-6 footer-links">
                                <h4>Useful Links</h4>
                                <ul>
                                    <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="Index">Home</a></li>
                                    <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="DisplayList">Mummy Records</a></li>
                                    <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="Supervised">Supervised Insights</a></li>
                                    <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="Unsupervised">Unsupervised Insights</a></li>
                                    <li><i class="bx bx-chevron-right"></i> <a asp-controller="Home" asp-action="Privacy">Privacy policy</a></li>
                                </ul>
                            </div>

                            <div class="col-lg-3 col-md-6 footer-links">
                                <h4>Learn More</h4>
                                <ul>
                                    <li><i class="bx bx-chevron-right"></i> <a href="https://www-sciencedirect-com.erl.lib.byu.edu/science/article/pii/S2352409X15000188">Info Article</a></li>
                                </ul>
                            </div>

                            <div class="col-lg-3 col-md-6 footer-contact">
                                <h4>Contact Us</h4>
                                <p>
                                    Tanner Building <br>
                                    Provo, UT 84602<br>
                                    United States <br><br>
                                    <strong>Phone:</strong> +1 801 422 1230<br>
                                    <strong>Email:</strong> byu@byu.edu<br>
                                </p>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="container">
            <div class="copyright">
                &copy; Copyright <strong><span>Hidayah</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
                <!-- All the links in the footer should remain intact. -->
                <!-- You can delete the links only if you purchased the pro version. -->
                <!-- Licensing information: https://bootstrapmade.com/license/ -->
                <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/hidayah-free-simple-html-template-for-corporate/ -->
                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a>
            </div>
        </div>
    </footer><!-- End Footer -->
    <!-- Vendor JS Files -->
    <div id="preloader"></div>
    <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>
    <script src="~/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.js"></script>
    <script src="~/vendor/glightbox/js/glightbox.js"></script>
    <script src="~/vendor/isotope-layout/isotope.pkgd.js"></script>
    <link href="~/vendor/swiper/swiper-bundle.min.css" rel="stylesheet" />
    <script src="~/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="~/vendor/php-email-form/validate.js"></script>
    <!-- Template Main JS File -->
    <script src="~/js/main.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>
